{
  "name": "react-nested-json-table",
  "version": "0.1.1",
  "description": "A simple React component that renders any deeply nested json into a collapsible table",
  "main": "dist/index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "dev": "npm run build && node server.js",
    "build": "npm run clean && npm run lint && webpack --colors",
    "clean": "rimraf build",
    "lint": "eslint src",
    "build-release": "babel src --out-dir dist && webpack --config webpack.prod.config.js",
	  "clean-release": "rimraf dist",
    "prerelease": "npm run lint && npm run test && npm run clean-release && npm run build-release",
    "release": "npm publish .",
    "test": "mocha src --compilers js:babel-register --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/once-ler/react-nested-json-table.git"
  },
  "keywords": [
    "reactjs",
    "FHIR"
  ],
  "author": "Henry Tao <htaox@hotmail.com> (http://github.com/once-ler)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/once-ler/react-nested-json-table/issues"
  },
  "homepage": "",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0-beta.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-0": "^6.0.0",
    "babel-register": "^6.6.0",
    "core-js": "^1.1.4",
    "css-loader": "^0.23.0",
    "enzyme": "^2.2.0",
    "eslint": "^1.7.1",
    "eslint-plugin-react": "^2.3.0",
    "expect": "^1.14.0",
    "jasmine-core": "^2.3.4",
    "mocha": "^2.4.5",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^1.2.7",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2",
    "webpack-hot-middleware": "^2.10.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0"
  }
}
